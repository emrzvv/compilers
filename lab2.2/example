func sumIntegers(a -> int; b -> int) -> int
	c -> char;
	c = 'A';
	A = [int 5], B = [int 10] -> [int];
	x, y = B[3], z = y -> int;
	y = y % 10;
	x = a * 2 + 10 / 5;
	if a < b then
		a = a + 1
	else
		b = b + 1
	endif;
	
	if a < b then
		a = a + 1
	elseif a == b + 1 then
		b = b + 1
	elseif a > b + 1 then
		a = a * 10
	else
		b = b * 10
	endif

endfunc


