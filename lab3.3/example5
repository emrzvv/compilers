func SumVectors(A, B -> [int]; E, F -> bool) -> [int]
	size = length(A) -> int;
	check size == length(B);
	C = [int size] -> [int];
	for i = 0 -> int to size - 1 do
		C[i] = A[i] + B[i]
	endfor;
	(*e = 'A' -> char;C[0] = e;*)
	SumVectors = C
endfunc

func main(a, b -> [int]) -> int
    F -> [[char]];
    (*SumVectors(a, b, c, d);*)
    c, d -> bool;
    SumVectors(a, b, c, d);
    main = 0
endfunc